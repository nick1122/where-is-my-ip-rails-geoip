<script>
handler = Gmaps.build('Google');
handler.buildMap({ provider: {}, internal: {id: 'map'}}, function(){
  markers = handler.addMarkers([
    {
      "lat": <%= @info.latitude %>,
      "lng": <%= @info.longitude %>,
      "picture": {
        "url": "https://cdn3.iconfinder.com/data/icons/10con/512/location_placeholder_pin_map-128.png",
        "width":  800,
        "height": 800
      },
      "infowindow": "hello!"
    }
  ]);
  handler.bounds.extendWith(markers);
  handler.fitMapToBounds();
});
</script>
